{
  "name": "autotester",
  "version": "0.1.0",
  "description": "Chrome extension allowing to develop and run automation tests right in browser",
  "author": {
    "name": "Vitaliy Potapov",
    "email": "noginsk@rambler.ru"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/autotester.git"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "dev": "node tasks/dev",
    "hub": "selenium-standalone start",
    "code": "node tasks/code",
    "test": "npm run test:l",
    "test:l": "node tasks/pack4test && node ./test/run local",
    "test:b": "node tasks/pack4test && node ./test/run browserstack",
    "test:s": "node tasks/pack4test && node ./test/run sauce",
    "test:y": "node tasks/pack4test && node ./test/run yandex",
    "build-master": "node tasks/build-master && node ./test/run local",
    "build-master-release": "npm run build-master && node tasks/copy2releases"
  },
  "dependencies": {},
  "devDependencies": {
    "archiver": "^1.1.0",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-react": "^6.16.0",
    "check-dependencies": "^1.0.1",
    "chnl": "^0.2.5",
    "chromedriver": "^2.24.1",
    "classnames": "^2.2.5",
    "codemirror": "^5.19.0",
    "cpx": "^1.5.0",
    "crx": "^3.0.3",
    "css-loader": "^0.25.0",
    "es6-promisify-all": "^0.1.0",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^3.7.0",
    "eslint-plugin-mocha": "^4.5.1",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.9.0",
    "fs-extra": "^0.30.0",
    "geckodriver": "^1.1.3",
    "ghooks": "^1.3.2",
    "got": "^6.5.0",
    "html5-fs": "^0.1.0",
    "http-browserify": "^1.7.0",
    "https-browserify": "0.0.1",
    "json-stringify-safe": "^5.0.1",
    "keymirror": "^0.1.1",
    "lodash.debounce": "^4.0.8",
    "madge": "^1.3.1",
    "marked": "^0.3.6",
    "mobx": "^2.5.2",
    "mobx-react": "^3.5.6",
    "mocha": "^3.1.0",
    "promises-aplus-tests": "^2.1.2",
    "raw-loader": "^0.5.1",
    "react": "^15.3.2",
    "react-codemirror": "^0.2.6",
    "react-dom": "^15.3.2",
    "react-mdl": "^1.7.2",
    "route-parser": "0.0.5",
    "selenium-standalone": "^5.7.2",
    "selenium-webdriver": "^3.0.0-beta-3",
    "shelljs": "^0.7.4",
    "style-loader": "^0.13.1",
    "then-chrome": "^1.0.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run code",
      "pre-push": "npm test"
    }
  },
  "keywords": [
    "selenium",
    "webdriver",
    "webdriverjs",
    "automation",
    "testing",
    "test",
    "acceptance",
    "tdd",
    "bdd",
    "chormedriver",
    "chrome"
  ],
  "license": "MIT"
}
